#RewriteCond %{HTTPS} off
#RewriteCond %{HTTP:X-Forwarded-Proto} !https
#RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

RewriteCond %{HTTP_HOST} ^www\.victor-azevedo\.me$
RewriteRule ^/?$ "https\:\/\/victor-azevedo\.me\/" [R=301,L]

RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://victor-azevedo.me/$1 [R=301,L]


<filesMatch ".(html|css|js)$">
Header set Cache-Control "max-age=604800, public"
</filesMatch>

<filesMatch ".(x-font-ttf|png|jpg|jpeg|svg|ttf)$">
Header set Cache-Control "max-age=2628000, public"
</filesMatch>

ErrorDocument 404 /redirect.html

Header set Access-Control-Allow-Origin "*"